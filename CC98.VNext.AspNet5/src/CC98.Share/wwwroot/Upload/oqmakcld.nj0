import urllib
import re

def gethtml(url):
    page=urllib.urlopen(url)
    html=page.read()
    return html

def getimage(html):
    reg=r'src="(\.+\.jpg)" pic.ext'
    imgre=re.compile(reg)
    imglist=re.findall(imgre,html)
    x=0
    for imgurl in imglist:
        urllib.urlretrieve(html,'%s.jpg'%x)
        x=x+1

